{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "spread": true
    }
  },
  "rules": {
    "arrow-parens": 0,
    "implicit-arrow-linebreak": 0,
    "no-console": 0,
    "function-paren-newline": 0,
    "no-underscore-dangle": 0,
    "operator-linebreak": 0,
    "curly": 0,
    "nonblock-statement-body-position": 0
  }
}
